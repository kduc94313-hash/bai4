print(" Sinh vien: Dau Van Khanh Duc")
print(" MSV : 245752021610021")

ds = ['banana', 'apple', 'cherry', 'date', 'fig']

print("Danh sách ban đầu:", ds)

ds.sort()
print("Sau khi sắp xếp (tăng dần):", ds)

ds.sort(reverse=True)
print("Sau khi sắp xếp (giảm dần):", ds)

so = [5, 2, 8, 1, 9]
print("\nDanh sách số:", so)
so.sort()
print("Sắp xếp số tăng dần:", so)
